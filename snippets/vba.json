	"Unit Test Template": {
		"prefix": "test",
		"description": "Unit test basic template",
		"body": [
			"Private Function TestList_${1:Identifier}() As TestResult",
			"Attribute TestList_$1.VB_Description = \"${2:Dosctring}.\"",
			"'   $2.",
			"    Dim tr As New TestResult",
			"",
			"'   Arrange",
			"    $0",
			"",
			"'   Act",
			"",
			"",
			"'   Assert",
			"",
			"",
			"Finally:",
			"    Set TestList_$1 = tr",
			"End Function"
		]
	}
}
